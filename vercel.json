{
  "version": 2,
  "builds": [
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/email-signup.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/recommendations-basic.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth-test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/wine-recommendations.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/recommendations-working.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/get-recommendations.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --target=node18 --external:rollup",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/email-signup",
      "dest": "/api/email-signup.js"
    },
    {
      "src": "/api/auth-test",
      "dest": "/api/auth-test.js"
    },
    {
      "src": "/api/recommendations",
      "dest": "/api/recommendations-working.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ]
}
